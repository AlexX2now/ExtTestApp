<Window x:Class="OOTVETINAVOPROSII.ResPage"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:OOTVETINAVOPROSII"
        mc:Ignorable="d"
        Title="ResPage" Height="450" Width="605">
    <Grid Background="#FFD6D6AB">
        <TextBlock x:Name="maintxt" HorizontalAlignment="Center" Margin="0,22,0,0" TextWrapping="Wrap" Text="Результаты" VerticalAlignment="Top" FontSize="24" Foreground="#FFDEA0F3" Height="40" Width="526"/>
        <TextBox x:Name="showres" HorizontalAlignment="Center" Margin="0,62,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="585" Height="295" Background="#FFFBF1D2" FontSize="20" VerticalScrollBarVisibility="Visible"/>
        <Button x:Name="esitbtn" Content="Вернуться" HorizontalAlignment="Center" Margin="0,372,0,0" VerticalAlignment="Top" Background="#FFFBF1D2" Foreground="#FFDEA0F3" FontSize="24" Width="311" Click="esitbtn_Click"/>

    </Grid>
</Window>
